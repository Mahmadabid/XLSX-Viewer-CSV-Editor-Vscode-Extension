{
  "name": "xlsx-viewer",
  "displayName": "XLSX Viewer CSV Editor",
  "description": "View XLSX files with styles, fonts and colors from excel files with multiple sheet support and edit CSV files in VS Code",
  "version": "1.1.2",
  "publisher": "muhammad-ahmad",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mahmadabid/XLSX-Viewer-CSV-Editor-Vscode-Extension"
  },
  "keywords": [
    "xlsx",
    "excel",
    "csv",
    "spreadsheet",
    "viewer",
    "editor",
    "multiple sheets",
    "style",
    "fonts",
    "theme"
  ],
  "categories": [
    "Other",
    "Data Science",
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "xlsxViewer.xlsx",
        "displayName": "XLSX Viewer",
        "selector": [
          {
            "filenamePattern": "*.xlsx"
          }
        ]
      },
      {
        "viewType": "xlsxViewer.csv",
        "displayName": "CSV Viewer",
        "selector": [
          {
            "filenamePattern": "*.csv"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "xlsx-viewer.goBackToTableView",
        "title": "Open in Table View",
        "category": "CSV",
        "icon": "$(table)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceExtname == '.csv' && activeEditor != 'xlsxViewer.csv'",
          "command": "xlsx-viewer.goBackToTableView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2",
    "esbuild": "^0.25.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "exceljs": "^4.3.0"
  },
  "license": "MIT",
  "icon": "icon.png"
}
