{
  "name": "xlsx-viewer",
  "displayName": "XLSX Viewer & CSV Editor",
  "description": "View XLSX files with styles, fonts and colors from excel files with multiple sheet support and view with table view and edit CSV files in VS Code. This is an open-source project.",
  "version": "1.5.0",
  "publisher": "muhammad-ahmad",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mahmadabid/XLSX-Viewer-CSV-Editor-Vscode-Extension"
  },
  "keywords": [
    "xlsx",
    "excel",
    "csv",
    "spreadsheet",
    "viewer",
    "editor",
    "multiple sheets",
    "style",
    "fonts",
    "theme"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "xlsxViewer.xlsx",
        "displayName": "XLSX Viewer",
        "selector": [
          {
            "filenamePattern": "*.xlsx"
          }
        ]
      },
      {
        "viewType": "xlsxViewer.csv",
        "displayName": "CSV Viewer",
        "selector": [
          {
            "filenamePattern": "*.csv"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "xlsx-viewer.goBackToTableView",
        "title": "Open in Table View",
        "category": "CSV",
        "icon": "resources/table.svg"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceExtname == '.csv' && activeEditor != 'xlsxViewer.csv'",
          "command": "xlsx-viewer.goBackToTableView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "esbuild": "^0.25.1",
    "eslint": "^9.24.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "chardet": "^2.1.0",
    "csv-parse": "^6.1.0",
    "exceljs": "^4.3.0",
    "iconv-lite": "^0.6.3"
  },
  "license": "MIT",
  "icon": "icon.png"
}
